{
  "printWidth": 120,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "arrowParens": "avoid",
  "proseWrap": "always",

  "Semicolons": true,
  "htmlWhitespaceSensitivity": "strict",
  "jsxSingleQuote": true,
  "quoteProps": "as-needed"
}
